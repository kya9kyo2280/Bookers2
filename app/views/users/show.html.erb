
<div class="container">
      <div class="row">

          <div class="col-lg-3" >
            <%= render 'books/list', book: @book %>
          </div>
          <div class="col-lg-9">
<h1>Books</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th> Title</th>
      <th> Body</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= link_to book.title, book_path(book.id) %></td>
      <td><%= book.body %></td>


    </tr>
    <% end %>
  </tbody>

</table>
</div>
</div>
</div>
